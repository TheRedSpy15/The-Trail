<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #36454f;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CareerController">
   <children>
      <Label layoutX="147.0" layoutY="14.0" prefHeight="57.0" prefWidth="306.0" text="Pick a career" textFill="WHITE">
         <font>
            <Font name="DejaVu Sans Condensed Bold" size="45.0" />
         </font>
      </Label>
      <HBox layoutX="47.0" layoutY="150.0" prefHeight="149.0" prefWidth="528.0">
         <children>
            <Button fx:id="carpenterBtn" mnemonicParsing="false" onAction="#carpenterHandle" prefHeight="99.0" prefWidth="168.0" text="Carpenter">
               <font>
                  <Font name="DejaVu Sans Bold" size="20.0" />
               </font>
            </Button>
            <Button fx:id="minerBtn" mnemonicParsing="false" onAction="#minerHandle" prefHeight="99.0" prefWidth="168.0" text="Miner">
               <font>
                  <Font name="DejaVu Serif Bold" size="20.0" />
               </font>
            </Button>
            <Button fx:id="farmerBtn" mnemonicParsing="false" onAction="#farmerHandle" prefHeight="99.0" prefWidth="168.0" text="Farmer">
               <font>
                  <Font name="DejaVu Sans Condensed Bold" size="20.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
