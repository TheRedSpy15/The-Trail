<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="319.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SettlementMenuController">
   <children>
      <Label fx:id="townLbl" layoutX="56.0" layoutY="36.0" prefHeight="45.0" prefWidth="489.0" text="You have come up to the town of: " textFill="PURPLE">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Separator layoutX="7.0" layoutY="83.0" prefHeight="3.0" prefWidth="588.0" />
      <Button fx:id="storeBtn" layoutX="30.0" layoutY="107.0" mnemonicParsing="false" onAction="#store" prefHeight="76.0" prefWidth="168.0" text="Use store">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="passBtn" layoutX="216.0" layoutY="107.0" mnemonicParsing="false" onAction="#keepGoing" prefHeight="76.0" prefWidth="168.0" text="Keep going">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="sellBtn" layoutX="403.0" layoutY="107.0" mnemonicParsing="false" onAction="#sell" prefHeight="76.0" prefWidth="168.0" text="Sell items">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="bountyBtn" layoutX="250.0" layoutY="215.0" mnemonicParsing="false" onAction="#claim" text="Claim bounty">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="bountyLbl" layoutX="81.0" layoutY="268.0" prefHeight="30.0" prefWidth="442.0" textFill="PURPLE">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
